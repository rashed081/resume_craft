@model PageNotFoundModel
@{
    ViewData["Title"] = "Not Found";
}

<header>
    <div class="text-center">
        <h2 class="text-danger">404</h2>
        <h2 class="text-secondary">Page @ViewData["Title"]!</h2>
        <hr>
        <div class="text-secondary">
            <h6>We're Sorry, Your Requested Page Could Not Be Found.</h6>
        </div>
    </div>
</header>